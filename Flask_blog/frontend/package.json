{
  "name": "flask-blog-frontend",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lh": "lighthouse http://localhost:5173/ --preset=mobile --output=json --output-path=./.lighthouse-temp.json",
    "lh:ci": "lhci autorun",
    "lint": "eslint src --ext .js,.ts,.vue || true",
    "openapi:download": "node scripts/download-openapi.mjs",
    "openapi:generate": "npm run openapi:download && openapi --input openapi.json --output src/generated --client axios --useUnionTypes true && node scripts/post-generate-patch.mjs",
    "errors:generate": "node scripts/generate-error-codes.mjs",
    "codegen": "npm run openapi:generate && npm run errors:generate",
    "governance:sync": "node scripts/sync-governance.mjs",
    "governance:check": "node scripts/check-governance-drift.mjs",
    "test": "vitest run",
    "test:ui": "vitest"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.3.2",
    "@shikijs/markdown-it": "^3.11.0",
    "@tailwindcss/nesting": "^0.0.0-insiders.565cd3e",
    "@tailwindcss/vite": "^4.1.12",
    "@tinymce/tinymce-vue": "^6.3.0",
    "@tiptap/extension-code-block-lowlight": "^2.26.1",
    "@tiptap/extension-image": "^2.26.1",
    "@tiptap/starter-kit": "^2.26.1",
    "@tiptap/vue-3": "^2.26.1",
    "axios": "^1.7.2",
    "dompurify": "^3.2.6",
    "easymde": "^2.18.0",
    "echarts": "^5.6.0",
    "element-plus": "^2.10.6",
    "highlight.js": "^11.11.1",
    "katex": "^0.16.22",
    "lowlight": "^3.3.0",
    "markdown-it": "^14.1.0",
    "markdown-it-anchor": "^9.2.0",
    "markdown-it-katex": "^2.0.3",
    "pinia": "^2.1.7",
    "shiki": "^3.9.2",
    "tinymce": "^8.0.2",
    "tiptap-extension-code-block-shiki": "^0.6.0",
    "turndown": "^7.2.0",
    "vditor": "^3.11.1",
    "vue": "^3.4.31",
    "vue-echarts": "^7.0.3",
    "vue-router": "^4.3.0",
    "web-vitals": "^4.2.4"
  },
  "devDependencies": {
    "@lhci/cli": "^0.13.0",
    "@tailwindcss/postcss": "^4.1.12",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vue/test-utils": "^2.4.6",
    "autoprefixer": "^10.4.21",
    "jsdom": "^26.1.0",
    "openapi-typescript-codegen": "^0.29.0",
    "postcss": "^8.5.6",
    "sass-embedded": "^1.91.0",
    "tailwindcss": "^4.1.12",
    "typescript": "^5.4.5",
    "unplugin-auto-import": "^20.0.0",
    "unplugin-vue-components": "^29.0.0",
    "vite": "^5.3.3",
    "vitest": "^1.6.0"
  }
}
